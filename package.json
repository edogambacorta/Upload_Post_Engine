{
  "name": "upload-post-engine",
  "version": "1.0.0",
  "description": "Mini-backend + React UI monolith for Upload-Post orchestration",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev:api": "tsx watch src/server.ts",
    "dev:web": "vite",
    "dev": "concurrently \"npm:dev:api\" \"npm:dev:web\"",
    "dev:clean": "powershell -ExecutionPolicy Bypass -File ./kill-port.ps1 && npm run dev",
    "build": "vite build && tsc",
    "start": "node dist/server.js",
    "kill-port": "powershell -ExecutionPolicy Bypass -File ./kill-port.ps1"
  },
  "dependencies": {
