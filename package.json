    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "fabric": "^6.9.0",
    "fuse.js": "^7.1.0",
    "html-to-image": "^1.11.13",
    "lucide-react": "^0.554.0",
    "openai": "^4.71.1",
    "react": "^18.3.1",
    "react-big-calendar": "^1.19.4",
